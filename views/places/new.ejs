<h1>Add a place!</h1>
<form id='newPlaceForm' action="/places" method="POST">
		<div>
			<label for="name">Name:</label>
			<input type="text" name="place[name]" id="name" autofocus>
		</div>
		<div>
			<label for="address">Address:</label>
			<input type="text" name="place[address]" id="address">
		</div>
		<div>
			<label for="city">City:</label>
			<input type="text" name="place[city]" id="city">
		</div>
		<div>
			<label for="state">State:</label>
			<input type="text" name="place[state]" id="state">
		</div>
		<div>
			<label for="phone">Phone:</label>
			<input type="text" name="place[phone]" id="phone">
		</div>
		<div>
			<input type="submit" value="Add!">
		</div>
	</form>
