<h1>About this place <%= place.name %></h1>

<p>
		<p>Address:
			<ul><%= place.address %></ul>
			<ul><%= place.city %>, <%= place.state %></ul>
		</p>
		<p>Phone:
			<ul><%= place.phone %></ul>
		</p>
		<p>Hours:</p>
			<ul></ul>
</p>
	<p>
		<h3><%= place.name %>'s reviews: </h3>
			<% reviews.forEach(function(review){ %>
			<p>
				<%= review.rating %> by <%=review.user_id.local.name%>
			</p>
			<p>
				<%= review.postDate %>
			</p>
			<p>
				<%= review.comments %>
			</p>
			<% }) %>
	</p>

		<a href="/places/<%= place.name%>/newReview">Add a new review</a>
	</p>
